# API Keys
API_KEY=your_api_key_here

# Storage Configuration
STORAGE_TYPE=s3  # Options: local, s3
LOCAL_STORAGE_PATH=static/images

# S3 Configuration
S3_ENDPOINT=
S3_REGION=
S3_ACCESS_KEY=
S3_SECRET_KEY=
S3_BUCKET=

# Custom Domain
CUSTOM_DOMAIN=

# Upload and Conversion Settings
# Maximum number of images allowed in a single upload (Need Self build default: 20)
# Image quality for WebP/AVIF conversion (1-100, default: 80)
# Number of parallel worker threads for batch processing (default: 4)
# Compression effort for WebP/AVIF conversion (1-10, default: 6)
# Force lossless WebP/AVIF conversion (default: false)
MAX_UPLOAD_COUNT=20
IMAGE_QUALITY=80
WORKER_THREADS=4
COMPRESSION_EFFORT=6
FORCE_LOSSLESS=false
